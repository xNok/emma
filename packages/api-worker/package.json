{
  "name": "@emma/api-worker",
  "version": "0.1.0",
  "description": "Cloudflare Worker for handling form submissions",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "generate:openapi": "node ./scripts/generate-openapi.mjs",
    "dev": "yarn generate:openapi && wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "tail": "wrangler tail",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@emma/shared": "workspace:*",
    "openapi-backend": "^5.10.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "wrangler": "^3.22.1",
    "yaml": "^2.5.1"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "api",
    "forms"
  ]
}
